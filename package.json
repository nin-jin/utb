{
  "name": "utb",
  "version": "1.0.26",
  "description": "Unified todomvc benchmark",
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "config": {
    "dest": "."
  },
  "scripts": {
    "release": "standard-version -a && npm run postversion",
    "postversion": "git push --follow-tags origin master && npm publish",
    "test": "exit 0",
    "build": "NODE_ENV=production rollup -c",
    "dev": "NODE_ENV=development rollup -c",
    "start": "static docs"
  },
  "author": "Stefan Zerkalica <zerkalica@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "http://github.com/zerkalica/utb.git"
  },
  "keywords": [
    "todomvc",
    "benchmark",
    "react",
    "inferno",
    "preact",
    "mobx",
    "reactive-di"
  ],
  "files": [
    "*.map",
    "*.js",
    "*.flow",
    ".flowconfig",
    "README.md",
    "LICENSE",
    "src",
    "flow-typed",
    "dist"
  ],
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.32",
    "@babel/plugin-external-helpers": "^7.0.0-beta.32",
    "@babel/plugin-proposal-class-properties": "^7.0.0-beta.32",
    "@babel/plugin-proposal-decorators": "^7.0.0-beta.32",
    "@babel/plugin-transform-flow-strip-types": "^7.0.0-beta.32",
    "@babel/plugin-transform-react-jsx": "^7.0.0-beta.32",
    "@babel/preset-es2015": "^7.0.0-beta.32",
    "@babel/register": "^7.0.0-beta.32",
    "babel-plugin-transform-metadata": "^4.0.1",
    "find-root": "^1.1.0",
    "flow-bin": "^0.59.0",
    "fs-extra": "^4.0.2",
    "node-static": "^0.7.10",
    "rimraf": "^2.6.2",
    "rollup": "^0.51.5",
    "rollup-plugin-babel": "^4.0.0-beta.0",
    "rollup-plugin-alias": "^1.4.0",
    "rollup-plugin-commonjs": "^8.2.6",
    "rollup-plugin-livereload": "^0.6.0",
    "rollup-plugin-node-globals": "^1.1.0",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-serve": "^0.4.2",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "rollup-plugin-uglify": "^2.0.1",
    "rollup-watch": "^4.3.1",
    "standard-version": "^4.2.0",
    "uglify-es": "^3.1.9"
  },
  "dependencies": {
    "inferno": "^3.10.1",
    "inferno-compat": "^3.10.1",
    "inferno-component": "^3.10.1",
    "inferno-create-element": "^3.10.1",
    "inferno-mobx": "^3.10.1",
    "lom_atom": "^3.0.2",
    "mobx": "^3.3.1",
    "mobx-preact": "^0.3.1",
    "mobx-react": "^4.3.4",
    "preact": "^8.2.6",
    "react": "^16.1.1",
    "react-dom": "^16.1.1",
    "reactive-di": "^6.0.0",
    "todomvc-common": "^1.0.4"
  }
}
