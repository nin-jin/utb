{
  "name": "utb",
  "version": "1.0.17",
  "description": "Unified todomvc benchmark",
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "config": {
    "dest": "."
  },
  "scripts": {
    "release": "UGLIFY=1 npm run build && standard-version -a && npm run postversion",
    "postversion": "git push --follow-tags origin master && npm publish",
    "test": "exit 0",
    "build": "rollup -c",
    "start": "static docs"
  },
  "author": "Stefan Zerkalica <zerkalica@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "http://github.com/zerkalica/utb.git"
  },
  "keywords": [
    "todomvc",
    "benchmark",
    "react",
    "inferno",
    "preact",
    "mobx",
    "reactive-di"
  ],
  "files": [
    "*.map",
    "*.js",
    "*.flow",
    ".flowconfig",
    "README.md",
    "LICENSE",
    "src",
    "flow-typed",
    "dist"
  ],
  "devDependencies": {
    "babel-plugin-external-helpers": "^7.0.0-alpha.19",
    "babel-plugin-transform-class-properties": "^7.0.0-alpha.19",
    "babel-plugin-transform-decorators": "^7.0.0-alpha.19",
    "babel-plugin-transform-metadata": "^4.0.1",
    "babel-preset-es2015": "^7.0.0-alpha.19",
    "babel-preset-flow": "^7.0.0-alpha.19",
    "babel-preset-react": "^7.0.0-alpha.19",
    "babel-preset-stage-0": "^7.0.0-alpha.19",
    "babel-register": "^7.0.0-alpha.19",
    "babelrc-rollup": "^3.0.0",
    "find-root": "^1.1.0",
    "flow-bin": "^0.54.1",
    "fs-extra": "^4.0.2",
    "node-static": "^0.7.10",
    "rimraf": "^2.6.1",
    "rollup": "^0.48.2",
    "rollup-plugin-alias": "^1.3.1",
    "rollup-plugin-babel": "=3.0.0-alpha.17",
    "rollup-plugin-commonjs": "^8.2.1",
    "rollup-plugin-livereload": "^0.6.0",
    "rollup-plugin-node-globals": "^1.1.0",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-serve": "^0.4.2",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "rollup-plugin-uglify": "^2.0.1",
    "rollup-watch": "^4.3.1",
    "standard-version": "^4.2.0",
    "uglify-es": "^3.1.0"
  },
  "dependencies": {
    "inferno": "^3.9.0",
    "inferno-compat": "^3.9.0",
    "inferno-component": "^3.9.0",
    "inferno-create-element": "^3.9.0",
    "inferno-mobx": "^3.9.0",
    "lom_atom": "^1.1.4",
    "mobx": "^3.3.0",
    "mobx-preact": "^0.3.1",
    "mobx-react": "^4.3.2",
    "preact": "^8.2.5",
    "react": "^15.6.2",
    "react-dom": "^15.6.2",
    "reactive-di": "^4.0.16",
    "todomvc-common": "^1.0.4"
  }
}
