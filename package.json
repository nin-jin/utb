{
  "name": "utb",
  "version": "1.0.39",
  "description": "Unified todomvc benchmark",
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "config": {
    "dest": "."
  },
  "scripts": {
    "release": "standard-version -a && npm run postversion",
    "postversion": "git push --follow-tags origin master && npm publish",
    "test": "exit 0",
    "build": "NODE_ENV=production rollup -c",
    "build.dev": "NODE_ENV=development rollup -c",
    "start": "static docs"
  },
  "author": "Stefan Zerkalica <zerkalica@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "http://github.com/zerkalica/utb.git"
  },
  "keywords": [
    "todomvc",
    "benchmark",
    "react",
    "inferno",
    "preact",
    "mobx",
    "reactive-di"
  ],
  "files": [
    "*.map",
    "*.js",
    "*.flow",
    ".flowconfig",
    "README.md",
    "LICENSE",
    "src",
    "flow-typed",
    "dist"
  ],
  "devDependencies": {
    "@babel/plugin-check-constants": "^7.0.0-beta.38",
    "@babel/core": "=7.0.0-beta.40",
    "@babel/plugin-external-helpers": "=7.0.0-beta.40",
    "@babel/plugin-proposal-class-properties": "=7.0.0-beta.40",
    "@babel/plugin-proposal-decorators": "=7.0.0-beta.40",
    "@babel/plugin-proposal-object-rest-spread": "=7.0.0-beta.40",
    "@babel/plugin-transform-flow-strip-types": "=7.0.0-beta.40",
    "@babel/plugin-transform-react-jsx": "=7.0.0-beta.40",
    "@babel/preset-es2015": "=7.0.0-beta.40",
    "@babel/register": "=7.0.0-beta.40",
    "find-root": "^1.1.0",
    "flow-bin": "^0.66.0",
    "fs-extra": "^5.0.0",
    "node-static": "^0.7.10",
    "rimraf": "^2.6.2",
    "rollup": "^0.56.3",
    "rollup-plugin-alias": "^1.4.0",
    "rollup-plugin-babel": "^4.0.0-beta.0",
    "rollup-plugin-commonjs": "^8.3.0",
    "rollup-plugin-livereload": "^0.6.0",
    "rollup-plugin-node-globals": "^1.1.0",
    "rollup-plugin-node-resolve": "^3.0.3",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-serve": "^0.4.2",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "rollup-plugin-uglify": "^3.0.0",
    "rollup-watch": "^4.3.1",
    "standard-version": "^4.3.0",
    "uglify-es": "^3.3.9"
  },
  "dependencies": {
    "inferno": "^4.0.6",
    "inferno-clone-vnode": "^4.0.6",
    "inferno-compat": "^4.0.6",
    "inferno-component": "^4.0.6",
    "inferno-create-element": "^4.0.6",
    "inferno-mobx": "^4.0.6",
    "inferno-redux": "^4.0.6",
    "lom_atom": "^4.0.0",
    "mobx": "^3.6.1",
    "mobx-preact": "^1.1.0",
    "mobx-react": "^4.4.2",
    "preact": "^8.2.7",
    "preact-redux": "^2.0.3",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.7",
    "reactive-di": "^7.0.0",
    "redux": "^3.7.2",
    "todomvc-common": "^1.0.4"
  }
}
